    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "postcss": "^8",
    "sass": "^1.79.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^2.1.2",
    "husky": "^8.0.0",
    "prettier": "^3.2.5",
    "lint-staged": "^13.1.0"
  },
  "lint-staged": {
